<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.web.WebView?>


<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ViewController.QuicklinksViewController">
    <center>
        <WebView fx:id="webview"/>
    </center>
    <bottom>
        <HBox spacing="10" alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
            <Button onAction="#zurueckGehen" minWidth="80" mnemonicParsing="false" text="Zurück"  />
            <Button onAction="#vorwärtsGehen" minWidth="80" mnemonicParsing="false" text="Vorwärts"  />
            <ProgressIndicator fx:id="progressIndicator"></ProgressIndicator>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </BorderPane.margin>
        </HBox>
    </bottom>
</BorderPane>
